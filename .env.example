# Database Configuration
MYSQL_HOST=localhost
MYSQL_USER=<your_username>
MYSQL_PASSWORD=<your_password>
MYSQL_DATABASE=campus_lens
MYSQL_PORT=3306

# --- DEVELOPMENT BYPASS ---
# This enables the bypass logic in app/api/dependencies.py
DEV_AUTH_BYPASS=true
DEV_AUTH_AS_ADMIN=true

# --- CLERK CONFIGURATION ---
# These are still needed by the dependency file, even if not used by the bypass
CLERK_JWKS_URL=<your_clerk_url>
CLERK_ISSUER=<your_issuer_url>
CLERK_VERIFY_AUD=false
CLERK_VERIFY_ISS=false
